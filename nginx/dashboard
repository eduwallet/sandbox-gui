server {
  listen 80;
  #server_name test-dashboard.pt-76.utr.surfcloud.nl;

  location / {
    proxy_pass http://localhost:8000;
  }

  location /.well-known/did.json {
    proxy_pass https://agent.dev.eduwallet.nl/dashboard/.well-known/did.json;
  }

  location /images/ {
    autoindex on;
    root /opt/eduWallet-test-dashboard/static;
  }
}
